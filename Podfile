project 'The Repo.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'The Repo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for The Repo
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'ProgressHUD'
  pod 'SDWebImage', '~> 5.0'
  pod 'lottie-ios'

  target 'The RepoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'The RepoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
